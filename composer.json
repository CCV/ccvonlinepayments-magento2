{
  "name": "ccv/magento2",
  "description": "CCVOnlinePayments for Magento2",
  "homepage": "https://www.ccv.eu/nl/betaaloplossingen/betaaloplossingen-online/ccv-online-payments/",
  "type": "magento2-module",
  "version": "1.1.0",
  "require": {
    "php": "~7.2||~7.3||~7.4",
    "ccv/lib": "^1.1",
    "magento/framework": "^102.0.0",
    "magento/module-payment": "^100.3.0"
  },
  "require-dev": {
    "ccv/images": "^1.1"
  },
  "autoload": {
    "files": [ "registration.php" ],
    "psr-4": {
      "CCVOnlinePayments\\Magento\\Controller\\": "Controller",
      "CCVOnlinePayments\\Magento\\": "src"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    },
    {
      "type": "path",
      "url": "../lib/"
    },
    {
      "type": "path",
      "url": "../images/"
    },
    {
      "type": "path",
      "url": "../magento/"
    }
  ]
}
