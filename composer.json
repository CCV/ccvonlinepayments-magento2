{
  "name": "ccvonlinepayments/magento2",
  "description": "CCVOnlinePayments for Magento2",
  "type": "magento2-module",
  "version": "1.0.0",
  "require": {
    "php": "~7.2||~7.3||~7.4",
    "ccvonlinepayments/lib": "^1.0",
    "magento/framework": "^102.0.0",
    "magento/module-payment": "^100.3.0"
  },
  "require-dev": {
    "ccvonlinepayments/images": "^1.0"
  },
  "autoload": {
    "files": [ "registration.php" ],
    "psr-4": {
      "CCVOnlinePayments\\Magento\\Controller\\": "Controller",
      "CCVOnlinePayments\\Magento\\": "src"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://repo.magento.com/"
    },
    {
      "type": "path",
      "url": "../../lib/"
    },
    {
      "type": "path",
      "url": "../../images/"
    },
    {
      "type": "path",
      "url": "../../magento/"
    }
  ]
}
